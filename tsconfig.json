{
    "compilerOptions": {
        "target": "ES2020",
        "moduleResolution": "node",
        "allowJs": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "declaration": true,
        "declarationDir": "./types",
        "lib": ["dom","ES2020"],
        "outDir": "output"
    },
    "exclude": [
        "node_modules",
        "*.js",
        "types/*",
        "output/*"
    ]
